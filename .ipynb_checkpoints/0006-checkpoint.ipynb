    "def sum_squares(N):\n",
    "    val = 0\n",
    "    for i in range(1,N+1):\n",
    "        val += i**2\n",
    "    return val\n",
    "\n",
    "def square_sums(N):\n",
    "    val = 0\n",
    "    for i in range(1,N+1):\n",
    "        val += i\n",
    "    return val**2\n",
    "\n",
    "def difference(N):\n",
    "    return square_sums(N) - sum_squares(N)\n",
    "\n",
    "difference(100)"
   ]
  }
