    "a = range(2,60)\n",
    "\n",
    "fib = [1,2]\n",
    "for i in a:\n",
    "    if fib[i-1]+fib[i-2] <= 4000000:\n",
    "        fib.append(fib[i-1]+fib[i-2])\n",
    "    else: \n",
    "        break\n",
    "total = 0\n",
    "for i in fib:\n",
    "    if i % 2 == 0:\n",
    "        total += i\n",
    "print(total)"
   ]
